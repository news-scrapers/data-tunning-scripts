os\n... ypermiteincrementar la cadencia de\ntirohasta los 600 disparos por minuto\n   VIENE DE LA PÁGINA ANTERIOR “Escomoellego,un\njugueteparaadultos” \n\t\t","url":"http://hemeroteca.lavanguardia.com/preview/2012/07/29/pagina-4/90128735/pdf.html","newspaper":"lavanguardia","scraper_id":"all-digitalocean-scraper","id":"73556602-6fe7-410f-6ac5-51578d1f3f3a","sentiment_analysis":{"score":2,"comparative":0,"tokens":[{"value":"4","negate":false},{"value":"lavanguardia","negate":false},{"value":"internacional","negate":false},{"value":"domingo","negate":false},{"value":"29","negate":false},{"value":"julio","negate":false},{"value":"2012","negate":false},{"value":"el","negate":false},{"value":"mercado","negate":false},{"value":"el","negate":false},{"value":"ar-15","negate":false},{"value":"provoca","negate":false},{"value":"un","negate":false},{"value":"cambio","negate":false},{"value":"en","negate":false},{"value":"el","negate":false},{"value":"negocio","negate":false},{"value":"al","negate":false},{"value":"atraer","negate":false},{"value":"clientes","negate":false},{"value":"más","negate":false},{"value":"jóvenes","negate":false},{"value":"yurbanos","negate":false},{"value":"consecuencias","negate":false},{"value":"de","negate":false},{"value":"la","negate":false},{"value":"matanza","negate":false},{"value":"de","negate":false},{"value":"colorado","negate":false},{"value":"“somosgentealaquesemanipulaconelmiedo”","negate":false},{"value":"la","negate":false},{"value":"conversación","negate":false},{"value":"habría","negate":false},{"value":"descon-","negate":false},{"value":"certado","negate":false},{"value":"aholmes","negate":false},{"value":"de","negate":false},{"value":"24","negate":false},{"value":"años","negate":false},{"value":"“seguridad","negate":false},{"value":"yseguridad”","negate":false},{"value":"repi-","negate":false},{"value":"te","negate":false},{"value":"kent","negate":false},{"value":"el","negate":false},{"value":"anfitrión","negate":false},{"value":"